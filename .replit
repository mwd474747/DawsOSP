modules = ["bash", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["glibcLocales", "libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DawsOS"

[[workflows.workflow]]
name = "DawsOS"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run dawsos/main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34109
externalPort = 3001

[[ports]]
localPort = 40523
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "dawsos/main.py", "--server.port=5000", "--server.address=0.0.0.0", "--server.headless=true"]
