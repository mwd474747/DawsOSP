{
  "dashboard": {
    "title": "DawsOS SLO Overview",
    "uid": "dawsos-slo",
    "tags": ["dawsos", "slo"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "API Latency p99 (SLO: ≤500ms)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(dawsos_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {
            "value": 0.5,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 2,
        "title": "API Latency p95 (SLO: ≤300ms)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dawsos_api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 latency",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "thresholds": [
          {
            "value": 0.3,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Request Rate by Pattern",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum by (pattern_id) (rate(dawsos_requests_total[5m]))",
            "legendFormat": "{{pattern_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(dawsos_request_errors_total[5m])) / sum(rate(dawsos_requests_total[5m]))",
            "legendFormat": "Error rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Error Rate"},
          {"format": "short"}
        ],
        "thresholds": [
          {
            "value": 0.01,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 5,
        "title": "Pack Freshness Status",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "dawsos_pack_freshness",
            "legendFormat": "{{pack_id}}",
            "refId": "A"
          }
        ],
        "mappings": [
          {"value": 0, "text": "WARMING"},
          {"value": 1, "text": "FRESH"},
          {"value": 2, "text": "ERROR"},
          {"value": 3, "text": "STALE"}
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "yellow"},
                {"value": 1, "color": "green"},
                {"value": 2, "color": "red"},
                {"value": 3, "color": "orange"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Pack Build Duration (SLO: ≤10min)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "dawsos_pack_build_duration_seconds",
            "legendFormat": "{{pack_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ],
        "thresholds": [
          {
            "value": 600,
            "colorMode": "critical",
            "op": "gt",
            "fill": true,
            "line": true
          }
        ]
      },
      {
        "id": 7,
        "title": "Agent Invocations by Capability",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (agent_name, capability) (rate(dawsos_agent_invocations_total[5m]))",
            "legendFormat": "{{agent_name}}.{{capability}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Invocations/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Circuit Breaker State",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "dawsos_circuit_breaker_state",
            "legendFormat": "{{agent_name}}",
            "refId": "A"
          }
        ],
        "mappings": [
          {"value": 0, "text": "CLOSED"},
          {"value": 1, "text": "OPEN"},
          {"value": 2, "text": "HALF_OPEN"}
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "red"},
                {"value": 2, "color": "yellow"}
              ]
            }
          }
        }
      }
    ]
  }
}
