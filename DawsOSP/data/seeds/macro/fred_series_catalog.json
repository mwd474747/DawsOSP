{
  "schema_version": "1.0",
  "series": {
    "T10Y2Y": {
      "desc": "10-Year Treasury Constant Maturity Minus 2-Year Treasury Constant Maturity",
      "transform": "raw",
      "units": "Percent",
      "frequency": "Daily"
    },
    "UNRATE": {
      "desc": "Unemployment Rate",
      "transform": "yoy_change",
      "units": "Percent",
      "frequency": "Monthly"
    },
    "CPIAUCSL": {
      "desc": "Consumer Price Index for All Urban Consumers: All Items in U.S. City Average",
      "transform": "yoy_change",
      "units": "Index 1982-1984=100",
      "frequency": "Monthly"
    },
    "GDP": {
      "desc": "Gross Domestic Product",
      "transform": "yoy_change",
      "units": "Billions of Dollars",
      "frequency": "Quarterly"
    },
    "PAYEMS": {
      "desc": "All Employees, Total Nonfarm",
      "transform": "yoy_change",
      "units": "Thousands of Persons",
      "frequency": "Monthly"
    },
    "INDPRO": {
      "desc": "Industrial Production: Total Index",
      "transform": "yoy_change",
      "units": "Index 2017=100",
      "frequency": "Monthly"
    },
    "HOUST": {
      "desc": "Housing Starts: Total: New Privately Owned Housing Units Started",
      "transform": "yoy_change",
      "units": "Thousands of Units",
      "frequency": "Monthly"
    },
    "BAA10Y": {
      "desc": "Moody's Seasoned Baa Corporate Bond Yield Relative to Yield on 10-Year Treasury Constant Maturity",
      "transform": "raw",
      "units": "Percent",
      "frequency": "Daily"
    }
  }
}
