{
  "id": "technical_analysis",
  "name": "Technical Analysis",
  "description": "Performs technical analysis with indicators and chart patterns",
  "triggers": ["technical", "chart", "rsi", "macd", "moving average", "support", "resistance"],
  "entities": ["SYMBOL"],
  "priority": 7,
  "steps": [
    {
      "agent": "data_harvester",
      "method": "process",
      "params": {
        "request": "Get {SYMBOL} price data with volume for last 6 months"
      },
      "output": "price_data"
    },
    {
      "agent": "pattern_spotter",
      "method": "process",
      "params": {
        "data": "{price_data}",
        "analysis_type": "technical",
        "indicators": ["RSI", "MACD", "MA50", "MA200", "Volume"]
      },
      "output": "technical_signals"
    },
    {
      "agent": "claude",
      "method": "process",
      "params": {
        "user_input": "Provide technical analysis for {SYMBOL} based on {technical_signals}: 1) Current trend 2) Key levels 3) Indicator signals 4) Entry/exit points 5) Risk/reward setup"
      },
      "output": "technical_report"
    }
  ],
  "response_type": "analysis",
  "response_template": "{technical_report}",
  "cache_ttl": 300
}