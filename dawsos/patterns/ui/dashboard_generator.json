{
  "id": "dashboard_generator",
  "name": "Dynamic Dashboard Generator",
  "description": "Generates personalized dashboard based on user patterns and preferences",
  "category": "ui",
  "triggers": ["dashboard", "home", "overview"],
  "workflow": [
    {
      "step": "get_ui_config",
      "action": "enriched_lookup",
      "params": {
        "data_type": "ui_configurations",
        "query": "dashboard_widgets"
      },
      "output": "widget_config"
    },
    {
      "step": "get_layout_template",
      "action": "enriched_lookup",
      "params": {
        "data_type": "ui_configurations",
        "query": "layout_templates.default_dashboard"
      },
      "output": "layout"
    },
    {
      "step": "generate_dashboard",
      "agent": "ui_generator",
      "params": {
        "component_type": "dashboard_widget",
        "data": {
          "widgets": "{widget_config}",
          "layout": "{layout}",
          "theme": "dark_theme"
        },
        "style": "grid"
      },
      "output": "dashboard_html"
    }
  ]
}